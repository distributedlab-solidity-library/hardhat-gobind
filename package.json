{
  "name": "@dlsl/hardhat-gobind",
  "version": "1.0.0",
  "description": "Generation of smart contract bindings for Golang",
  "scripts": {
    "install": "npx husky install",
    "compile": "tsc --build .",
    "build-local": "npm run compile && npm run lint-fix && bash ./scripts/build-local.sh",
    "clean": "bash ./scripts/clean.sh",
    "test": "mocha --config .mocharc.json --recursive test/**/*.test.ts --exit",
    "lint-fix": "prettier --write \"./**/*.{ts,js,sol}\"",
    "publish-to-npm": "npm run compile && npm run lint-fix && bash ./scripts/publish.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distributedlab-solidity-library/hardhat-gobind.git"
  },
  "keywords": [
    "ethereum",
    "solidity",
    "smart-contracts",
    "go-bindings",
    "hardhat",
    "hardhat-plugin",
    "distributedlab"
  ],
  "author": "Distributed Lab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/distributedlab-solidity-library/hardhat-gobind/issues"
  },
  "homepage": "https://github.com/distributedlab-solidity-library/hardhat-gobind#readme",
  "peerDependencies": {
    "hardhat": "^2.10.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "hardhat": "^2.10.0",
    "husky": "^7.0.2",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.1.1",
    "ts-node": "^8.1.0",
    "typescript": "~4.5.2"
  }
}